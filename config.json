{
    "name": "Kilosort4_param_sweep",
    "algorithm": "bayes",
    "spec": {
        "metric": "units",
        "objective": "maximize"
    },
    "parameters": {
        "batch_size": {"type": "discrete", "values": [6000]},
        "nblocks": {"type": "discrete", "values": [1]},
        "Th_universal":{"type": "discrete", "values": [9]},
        "Th_learned": {"type": "discrete", "values": [8]},
        "do_CAR": {"type": "categorical","values": ["True"]},
        "invert_sign": {"type": "categorical","values": ["False"]},
        "nt": {"type": "discrete", "values": [61]},
        "nskip": {"type": "discrete", "values": [25]},
        "binning_depth": {"type": "discrete", "values": [5]},
        "sig_interp": {"type": "discrete", "values": [20]},
        "nt0min": {"type": "integer", "scaling_type": "uniform", "min": 5, "max": 20},
        "dmin": {"type": "float", "scaling_type": "uniform", "min": 18, "max": 70},
        "dminx": {"type": "discrete", "values": [17.5]},
        "min_template_size": {"type": "float", "scaling_type": "uniform", "min": 5, "max": 20},
        "template_sizes": {"type": "integer", "scaling_type": "uniform", "min": 1, "max": 10},
        "nearest_chans": {"type": "integer", "scaling_type": "uniform", "min": 1, "max": 20},
        "nearest_templates": {"type": "integer", "scaling_type": "uniform", "min": 1, "max": 200},
        "max_channel_distance": {"type": "float", "scaling_type": "uniform", "min": 1, "max": 100},
        "templates_from_data": {"type": "categorical","values": ["True"]},
        "n_templates": {"type": "integer", "scaling_type": "uniform", "min": 3, "max": 20},
        "n_pcs": {"type": "integer", "scaling_type": "uniform", "min": 3, "max": 10},
        "acg_threshold": {"type": "discrete", "values": [0.1]},
        "ccg_threshold": {"type": "discrete", "values": [0.1]},
        "cluster_downsampling":{"type": "discrete", "values": [20]},
        "cluster_pcs": {"type": "discrete", "values": [64]},
        "duplicate_spike_bins": {"type": "discrete", "values": [15]},
        "keep_good_only": {"type": "categorical","values": ["False"]},
        "do_correction": {"type": "categorical","values": ["False"]},
        "save_extra_kwargs": {"type": "categorical","values": ["False"]},
        "skip_kilosort_preprocessing": {"type": "categorical","values": ["False"]},
        "scaleproc": {"type": "discrete", "values": [200]},
        "artifact_threshold": {"type": "float", "scaling_type": "uniform", "min": 0, "max": 6000},
        "whitening_range": {"type": "discrete", "values": [32]},
        "Th_single_ch": {"type": "discrete", "values": [6]}
    }
}